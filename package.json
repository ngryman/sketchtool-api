{
  "name": "sketchtool-api",
  "version": "0.0.0",
  "description": "JavaScript API for sketchtool.",
  "author": "Nicolas Gryman <ngryman@gmail.com> (http://ngryman.sh/)",
  "license": "MIT",
  "repository": "ngryman/sketchtool-api",
  "main": "index.js",
  "files": [
    "index.js"
  ],
  "scripts": {
    "test": "eslint {,lib/,test/}*.js && nyc ava",
    "dev": "npm run test -- --watch",
    "coverage": "nyc report --reporter=text-lcov | codecov",
    "see-coverage": "nyc report --reporter=html && open coverage/index.html",
    "check-coverage": "nyc check-coverage --lines 95 --functions 95 --branches 95"
  },
  "precommit": [
    "test",
    "check-coverage"
  ],
  "ava": {
    "require": [
      "babel-register"
    ]
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "keywords": [
    "sketch",
    "sketchtool",
    "api",
    "dump"
  ],
  "dependencies": {
    "mem": "^1.1.0"
  },
  "devDependencies": {
    "ava": "^0.17.0",
    "codecov.io": "^0.1.6",
    "eslint": "^3.1.1",
    "eslint-config-ngryman": "^1.4.0",
    "nyc": "^9.0.1",
    "pre-commit": "^1.1.3"
  }
}
